public class GetMinVal {
	public static void main(String[] args) 
	{
		
		//This read the input from the console
		Scanner userInput = new Scanner(System.in);
		
		//This declare the var
		int MinVal = -1;
		int MaxVal = 99;
		int counter;
		
		// for loop
		for (counter = 0; counter < 10; counter++)
		{
			
			//if array value is greater than max value set it to array value
			if(MinVal > ArrayOfIntergers[counter])
			{
				MinVal = ArrayOfIntergers[counter];
			}
			else ( MaxVal < ArrayOfIntergers[counter])
			{
				MaxVal = ArrayOfIntergers[counter];
			}
			
				// declare int
	int MAX_ARRAY_SIZE = 10;
	int MAX_RANDOM_NUM = 99;
	int[] arrayOfIntergers = new int[MAX_ARRAY_SIZE];
	int counter;
	int randomNumber;
	int minValue;
	int maxValue;
	
	// generate a new random number
	Random randNumGenerator = new Random();
	
	//for loop
	for ( counter = 0; counter < MAX_ARRAY_SIZE; counter++ ){
		
		//generate a new random num
		randomNumber = randNumGenerator(MAX_RANDOM_NUM + 1 );
		arrayOfIntergers[counter] = randomNumber;
	}
	// calling on function
	minValue = GetMinVal(randNumber);
	
	//Display the value
	System.out.println(" The Min " + minVal);
	System.out.println((" The Max is " + MaxVal);)
		}
		//This return  the max valrable
		return MinVal;
	}
}
